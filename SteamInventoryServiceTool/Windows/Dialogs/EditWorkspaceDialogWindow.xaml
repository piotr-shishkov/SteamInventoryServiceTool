<Window x:Class="SteamInventoryServiceTool.Windows.Dialogs.EditWorkspaceDialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:SteamInventoryServiceTool.Windows.Dialogs"
        mc:Ignorable="d"
        Title="Edit Workspace" Height="180" Width="300"
        ResizeMode="NoResize" WindowStyle="SingleBorderWindow" Icon="/Resources/AppLogo_Small.png">
    <StackPanel Margin="10">
        <TextBlock Text="Workspace Name:" Margin="0 0 0 5"/>
        <TextBox x:Name="WorkspaceNameTextBox" Margin="0 0 0 5"/>
        <TextBlock Text="App ID:" Margin="0 0 0 5"/>
        <TextBox x:Name="AppIdTextBox" Margin="0 0 0 10"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Content="OK" Width="75" Margin="0 0 0 0" Click="OK_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
